{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++-11 compilar archivo activo",
            "command": "/usr/bin/g++-11",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}", "${fileDirname}/analizador.cpp", "${fileDirname}/structs.cpp",
                "${fileDirname}/execute.cpp", "${fileDirname}/lexico.cpp",
                "${fileDirname}/mkdisk.cpp", "${fileDirname}/rmdisk.cpp",
                "${fileDirname}/fdisk.cpp", "${fileDirname}/mount.cpp",
                "${fileDirname}/unmount.cpp", "${fileDirname}/rep.cpp",
                "${fileDirname}/mkfs.cpp", "${fileDirname}/login.cpp",
                "${fileDirname}/logout.cpp", "${fileDirname}/mkgrp.cpp",
                "${fileDirname}/mkusr.cpp", "${fileDirname}/rmusr.cpp",
                "${fileDirname}/rmgrp.cpp", "${fileDirname}/chgrp.cpp",
                "${fileDirname}/pause.cpp", "${fileDirname}/chmod.cpp",
                "${fileDirname}/mkdir.cpp", "${fileDirname}/mkfile.cpp",
                "${fileDirname}/loss.cpp", "${fileDirname}/chown.cpp",
                "${fileDirname}/edit.cpp", "${fileDirname}/remove.cpp",
                "${fileDirname}/copy.cpp", "${fileDirname}/move.cpp",
                "${fileDirname}/find.cpp", "${fileDirname}/rename.cpp",
                "${fileDirname}/cat.cpp", "${fileDirname}/recovery.cpp",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Tarea generada por el depurador."
        }
    ],
    "version": "2.0.0"
}