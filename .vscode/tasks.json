{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++-11 compilar archivo activo",
            "command": "/usr/bin/g++-11",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}", "${fileDirname}/analizador.cpp", "${fileDirname}/structs.cpp",
                "${fileDirname}/lexico.cpp", "${fileDirname}/execute.cpp",
                "${fileDirname}/mkdisk.cpp", "${fileDirname}/rmdisk.cpp",
                "${fileDirname}/fdisk.cpp", "${fileDirname}/mount.cpp",
                "${fileDirname}/unmount.cpp", "${fileDirname}/rep.cpp",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Tarea generada por el depurador."
        }
    ],
    "version": "2.0.0"
}